<?xml version="1.0" encoding="utf-8"?>
<com.adslinfosoft.softberry.adapter.GameItemView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minWidth="@dimen/card_min_width"
    android:padding="8dp">

    <TextView
        android:id="@+id/text_job_no_val"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_column="1"
        android:fontFamily="@font/sanspro_regular"
        android:layout_row="0"
        android:layout_columnWeight="100"
        android:textColor="@color/text_color"
        tools:text="1000000000000000000000000000000000000000000000000000000"/>

    <TextView
        android:id="@+id/text_job_no"
        android:layout_height="wrap_content"
        android:layout_gravity="fill"
        android:layout_column="0"
        android:layout_width="wrap_content"
        android:layout_columnWeight="1"
        android:layout_row="0"
        android:gravity="center_vertical"
        tools:text="Media Model"
        android:fontFamily="@font/sanspro_regular"
        android:textColor="@color/text_color" />

</com.adslinfosoft.softberry.adapter.GameItemView>